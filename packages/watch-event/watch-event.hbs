<{{elementName}} {{{attr attributes}}}>
	{{#if img}}<{{img.elementName}} {{{attr img.attributes}}}/>{{/if}}
	<div class="ocl-watch-event__content">
		<{{title.elementName}} {{{attr title.attributes}}}>
			{{{title.body}}}
		</{{title.elementName}}>
		<{{subtitle.elementName}} {{{attr subtitle.attributes}}}>
			{{{subtitle.body}}}
		</{{subtitle.elementName}}>
		<{{detail.elementName}} {{{attr detail.attributes}}}>
			{{{detail.body}}}
		</{{detail.elementName}}>
	</div>
</{{elementName}}>