<{{elementName}} {{{attr attributes}}}>
	<div class="ocl-card__header ocl-assignment-card__header">
        <{{assigned.elementName}} {{{attr assigned.attributes}}}>Assigned: 11/22/17</{{assigned.elementName}}>
        <{{due.elementName}} {{{attr due.attributes}}}>Due: 11/22/17</{{due.elementName}}>
        <{{completed.elementName}} {{{attr completed.attributes}}}>Completed: 11/22/17</{{completed.elementName}}>
	</div>
	<div class="ocl-card__body ocl-assignment-card__body">
		{{#if title}}
		<{{title.elementName}} {{{attr title.attributes}}}>Assignment Title</{{title.elementName}}>
		{{/if}}
		{{#if assignment-list}}
		<ul class="ocl-assignment-card__list">
		{{#each assignment-list}}
			<li class="ocl-assignment-card__list-item">
				{{{icon}}}<h3 class="ocl-assignment-card__list-item-title">{{title}}</h3>
			</li>
		{{/each}}
		</ul>
		{{/if}}
		<a href="#" class="ocl-assignment-card__details">Details</a>
	</div>
	<div class="ocl-card__footer ocl-assignment-card__footer">
		{{#each buttons.left}}
			<a href="#" {{{attr this.attributes}}}>{{this.title}}</a>
		{{/each}}
		<a href="#" class="ocl-assignment-card__footer-trash"><i class="fa fa-trash" aria-hidden="true"></i></a>
	</div>
</{{elementName}}>