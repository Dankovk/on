<{{elementName}} {{{attr attributes}}}>
	<{{header.elementName}} {{{attr header.attributes}}}>
		{{{header.body}}}
	</{{header.elementName}}>

	{{#if questions-clean}}
		<{{questions-clean.elementName}} {{{attr questions-clean.attributes}}}>

			<label class="ocl-card ocl-quiz-question-option-card">
				<input type="radio" class="ocl-quiz-question-option-card__input" name="ocl-quiz-question-option-card-name"/>
			<div class="ocl-quiz-question-option-card__content">
				<div class="ocl-card__header ocl-quiz-question-option-card__header">
					<div class="ocl-quiz-question-option-card__header__title">C</div>
					<div class="ocl-quiz-question-option-card__header__correct-icon"><i class="fa fa-check" aria-hidden="true"></i></div>
					<div class="ocl-quiz-question-option-card__header__close-icon"><i class="fa fa-close" aria-hidden="true"></i></div>
				</div>
				{{#if img}}<{{img.elementName}} {{{attr img.attributes}}}/>{{/if}}
				<div class="ocl-card__body ocl-quiz-question-option-card__body">All the way back</div>
				<div class="ocl-card__footer ocl-quiz-question-option-card__footer"> <div class="ocl-quiz-question-option-card__footer__indicator"></div></div>
				</div>
			</label>

			<label class="ocl-card ocl-quiz-question-option-card">
				<input type="radio" class="ocl-quiz-question-option-card__input" name="ocl-quiz-question-option-card-name"/>
			<div class="ocl-quiz-question-option-card__content">
				<div class="ocl-card__header ocl-quiz-question-option-card__header">
					<div class="ocl-quiz-question-option-card__header__title">C</div>
					<div class="ocl-quiz-question-option-card__header__correct-icon"><i class="fa fa-check" aria-hidden="true"></i></div>
					<div class="ocl-quiz-question-option-card__header__close-icon"><i class="fa fa-close" aria-hidden="true"></i></div>
				</div>
				{{#if img}}<{{img.elementName}} {{{attr img.attributes}}}/>{{/if}}
				<div class="ocl-card__body ocl-quiz-question-option-card__body">On the front half</div>
				<div class="ocl-card__footer ocl-quiz-question-option-card__footer"> <div class="ocl-quiz-question-option-card__footer__indicator"></div></div>
				</div>
			</label>

			<label class="ocl-card ocl-quiz-question-option-card">
				<input type="radio" class="ocl-quiz-question-option-card__input" name="ocl-quiz-question-option-card-name"/>
			<div class="ocl-quiz-question-option-card__content">
				<div class="ocl-card__header ocl-quiz-question-option-card__header">
					<div class="ocl-quiz-question-option-card__header__title">C</div>
					<div class="ocl-quiz-question-option-card__header__correct-icon"><i class="fa fa-check" aria-hidden="true"></i></div>
					<div class="ocl-quiz-question-option-card__header__close-icon"><i class="fa fa-close" aria-hidden="true"></i></div>
				</div>
				{{#if img}}<{{img.elementName}} {{{attr img.attributes}}}/>{{/if}}
				<div class="ocl-card__body ocl-quiz-question-option-card__body">At the front edge, so that you are almost falling forward</div>
				<div class="ocl-card__footer ocl-quiz-question-option-card__footer"> <div class="ocl-quiz-question-option-card__footer__indicator"></div></div>
				</div>
			</label>
		</{{questions-clean.elementName}}>
	{{/if}}

