	<{{answer.elementName}} {{{attr answer.attributes}}}>
		<{{answer.details.elementName}} {{{attr answer.details.attributes}}}>
			{{{answer.details.body}}}
		</{{answer.details.elementName}}>
		<button class="ocl-button">
			<span class="ocl-button__label">Next Question</span>
			<span class="fa fa-chevron-right"></span>
		</button>
