<{{elementName}} {{{attr attributes}}}>
    <div class="ocl-card__header ocl-module-card__header ocl-quiz-card__header">
        <div class="ocl-module-card__header-content ocl-quiz-card__header-content">
            <a href="#" class="ocl-module-card__image">
                <img class="ocl-module-card__image-element" src="http://placehold.it/350x150" alt="">
                <div class="ocl-button">
                    <span>Quiz</span>
                </div>
            </a>
        </div>
        <div class="ocl-card__body module-card__body ocl-quiz-card__body">
            <div class="ocl-card__body-content ocl-module-card__body-content ocl-quiz-card__body-content">
                <a href="#" class="ocl-module-card__link">
                    <h3 class="ocl-module-card__title">
                        Sitting position
                    </h3>
                </a>
                <{{details.elementName}} {{{attr details.attributes}}}>
                    {{{details.body}}}
                </{{details.elementName}}>
            </div>
        </div>
    </div>
</{{elementName}}>