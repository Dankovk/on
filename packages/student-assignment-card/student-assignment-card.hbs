<{{elementName}} {{{attr attributes}}}>
	<div class="ocl-card__header ocl-assignment-card__header">
		<{{assigned.elementName}} {{{attr assigned.attributes}}}>{{assigned.body}}</{{assigned.elementName}}>
		<{{due.elementName}} {{{attr due.attributes}}}>{{due.body}}</{{due.elementName}}>
	</div>
	<div class="ocl-card__body ocl-assignment-card__body">
		{{#if title}}
		<{{title.elementName}} {{{attr title.attributes}}}>Assignment Title</{{title.elementName}}>
		{{/if}}
		{{#if assignment-list}}
		<ul class="ocl-assignment-card__list">
		{{#each assignment-list}}
			<li class="ocl-assignment-card__list-item">
				{{{icon}}}
				<div class="ocl-assignment-card-list-item-title__content">
					<h3 class="ocl-assignment-card__list-item-title">{{title}}</h3>
					<div class="ocl-assignment-card__list-item-details">{{details}}</div>
				</div>
			</li>
		{{/each}}
		</ul>
		{{/if}}
	</div>
	<div class="ocl-card__footer ocl-assignment-card__footer ocl-student-assignment-card__footer">
		<{{footer.title.elementName}} {{{attr footer.title.attributes}}}>{{footer.title.body}}</{{footer.title.elementName}}>
		<{{footer.content.elementName}} {{{attr footer.content.attributes}}}>{{footer.content.body}}</{{footer.content.elementName}}>
	</div>
</{{elementName}}>