<{{elementName}} {{{attr attributes}}}>
    <span class="ocl-drawer-nav__user">
        Signed&nbsp;in&nbsp;as:
        Barbara&nbsp;Thompson
    </span>
    <ul class="ocl-drawer-nav__primary-list">
        {{#each primaryLink}}
            <li class="ocl-drawer-nav__primary-list-item">
                <a href="" class="ocl-drawer-nav__primary-link">
                    <div class="ocl-drawer-nav__link-icon ocl-drawer-nav__link-icon--left">
                        {{{this.icon}}}
                    </div>
                    {{{this.text}}}
                    <div class="ocl-drawer-nav__link-icon ocl-drawer-nav__link-icon--right">
                        <i class="fa fa-chevron-right" aria-hidden="true"></i>
                    </div>
                </a>
            </li>
        {{/each}}
    </ul>
    <ul class="ocl-drawer-nav__secondary-list">
        {{#each secondaryLink}}
            <li class="ocl-drawer-nav__secondary-list-item">
                <a href="" class="ocl-drawer-nav__secondary-link">{{{this.text}}}</a>
            </li>
        {{/each}}
    </ul>
</{{elementName}}>
