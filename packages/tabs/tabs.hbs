<{{elementName}} {{{attr attributes}}}>

<{{controls.elementName}} {{{attr controls.attributes}}}>

	{{#each controls.tabs}}
		<{{this.elementName}} {{{attr this.attributes}}}>
			<{{this.input.elementName}} {{{attr this.input.attributes}}}/>
			<{{this.content.elementName}} {{{attr this.content.attributes}}}> {{{this.content.body}}} </{{this.content.elementName}}>
		</{{this.elementName}}>
	{{/each}}
</{{controls.elementName}}>

<{{panels.elementName}} {{{attr panels.attributes}}}>
	{{#each panels.panels}}
		<{{this.elementName}} {{{attr this.attributes}}}>
			{{{this.body}}}
		</{{this.elementName}}>
	{{/each}}
</{{panels.elementName}}>

</{{elementName}}>
