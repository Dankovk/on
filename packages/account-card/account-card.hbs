<{{elementName}} {{{attr attributes}}}>
    <div class="ocl-card__header ocl-account-card__header">
        <div class="ocl-card__header-content ocl-account-card__header-content">
            <h2 class="ocl-account-card__title">
                Account Plan
            </h2>
        </div>
    </div>
    <div class="ocl-card__body ocl-account-card__body">
        <div class="ocl-account-card__current-level">
            <h3 class="ocl-account-card__level-title ocl-account-card__current-level-title">
                Teacher
            </h3>
            <h4 class="ocl-account-card__level-subtitle ocl-account-card__current-level-subtitle">
                Up to 3 students
            </h4>
            <h5 class="ocl-account-card__level-detail ocl-account-card__current-level-detail">
                $XX/month
            </h5>
        </div>
        <div class="ocl-account-card__change-level">
            <h3 class="ocl-account-card__level-title ocl-account-card__change-level-title">
                Update account plan
            </h3>
            <h4 class="ocl-account-card__level-subtitle ocl-account-card__change-level-label">
                Billing date:
            </h4>
            <h5 class="ocl-account-card__level-detail ocl-account-card__change-level-detail">
                15th of the month
            </h5>
            <div class="ocl-account-card__change-level-select">
                <select name="" id="">
                    <option value="x">Teacher3 $xx/month</option>
                    <option value="x">Teacher3 $xx/month</option>
                    <option value="x">Teacher3 $xx/month</option>
                </select>
            </div>
            <div class="ocl-button ocl-account-card__change-level-button">
                <div class="ocl-button">
                    <span>Unlock Account Plans</span>
                </div>
            </div>
        </div>
    </div>
</{{elementName}}>